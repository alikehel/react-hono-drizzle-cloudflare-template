{
    "name": "react-hono-authjs-drizzle-cloudflare-template",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "turbo build",
        "dev": "turbo dev",
        "web": "pnpm --filter web",
        "api": "pnpm --filter api",
        "typecheck": "turbo typecheck",
        "lint": "biome check .",
        "lint:fix": "biome check . --write",
        "lint:fix:unsafe": "biome check . --write --unsafe",
        "prepare": "husky",
        "commitlint": "commitlint --edit"
    },
    "lint-staged": {
        "**/*": ["biome check . --write"]
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.3",
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "turbo": "^2.1.2",
        "typescript": "^5.6.2"
    },
    "engines": {
        "node": ">=20",
        "pnpm": ">=9"
    },
    "packageManager": "pnpm@9.12.1"
}
