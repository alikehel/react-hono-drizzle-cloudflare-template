{
    "name": "@react-hono-authjs-drizzle-cloudflare-template/api",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "pnpm wrangler dev --live-reload --env dev",
        "typecheck": "tsc --noEmit",
        "deploy": "wrangler deploy --minify",
        "deploy:stage": "pnpm wrangler deploy --minify --env stage --var GITHUB_REF_NAME:$GITHUB_REF_NAME --var GITHUB_SHA:$GITHUB_SHA",
        "deploy:prod": "pnpm wrangler deploy --minify --env prod --var GITHUB_REF_NAME:$GITHUB_REF_NAME --var GITHUB_SHA:$GITHUB_SHA"
    },
    "dependencies": {
        "@hono/zod-openapi": "^0.16.4",
        "hono": "^4.6.4",
        "hono-pino": "^0.3.0",
        "pino": "^9.5.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240925.0",
        "wrangler": "^3.79.0",
        "pino-pretty": "^11.3.0"
    }
}
