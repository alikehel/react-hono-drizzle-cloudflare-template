{
    "name": "@react-hono-authjs-drizzle-cloudflare-template/web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "typecheck": "tsc -b --noEmit",
        "deploy:stage": "pnpm wrangler pages deploy ./dist --project-name react-hono-authjs-drizzle-cloudflare-template-web-stage --branch \"stage\" --commit-hash \"$GITHUB_SHA\" --commit-message \"stage deployment\"",
        "deploy:prod": "pnpm wrangler pages deploy ./dist --project-name react-hono-authjs-drizzle-cloudflare-template-web-prod",
        "generate:api:local": "openapi-typescript http://127.0.0.1:8787/api -o ./src/api.d.ts",
        "generate:api:prod": "openapi-typescript https://prod.api.react-hono-authjs-drizzle-cloudflare-template.alikehel.dev/api -o ./src/api.d.ts"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.59.16",
        "openapi-fetch": "^0.13.0",
        "openapi-react-query": "^0.2.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240925.0",
        "@types/react": "^18.3.10",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.2",
        "openapi-typescript": "^7.4.2",
        "typescript": "^5.6.3",
        "vite": "^5.4.8",
        "wrangler": "^3.79.0"
    }
}
